{
  "version": 2,
  "name": "test-storyblock",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" },
      "env": {
        "STORYBLOK_TOKEN": "@storyblok_token"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "404.html" }
  ]
}
